<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bluebackground"
    tools:context=".UserProfile">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="138dp"
        android:layout_height="142dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        app:srcCompat="@drawable/profile" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView18"
        android:layout_alignParentTop="true"
        android:layout_marginTop="196dp"
        android:text="Simon Delprado"
        android:textColor="#000000"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="match_parent"
        android:layout_height="112dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="0dp"
        app:srcCompat="@drawable/medicalbackground" />

    <Button
        android:id="@+id/button45"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="291dp"
        android:background="#f9f1f1"
        android:text="Activities"
        android:textSize="15dp"
        android:textStyle="bold" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="345dp"
        android:background="#f2f1f1" />

    <Button
        android:id="@+id/button49"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_below="@+id/view2"
        android:background="#f9f1f1"
        android:text="Information"
        android:textSize="15dp"
        android:textStyle="bold" />

    <View
        android:id="@+id/view3"
        android:layout_width="383dp"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="396dp"
        android:background="#f2f1f1" />

    <Button
        android:id="@+id/button50"
        android:layout_width="match_parent"
        android:layout_height="55dp"
        android:layout_above="@+id/imageView25"
        android:layout_marginBottom="0dp"
        android:background="#f9f1f1"
        android:text="Archive"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="452dp"
        android:background="#f2f1f1" />

    <Button
        android:id="@+id/button56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/button45"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="16dp"
        android:background="@android:color/transparent"
        android:text=">" />

    <Button
        android:id="@+id/button57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/button56"
        android:layout_alignBottom="@+id/view3"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="15dp"
        android:background="@android:color/transparent"
        android:text=">" />

    <Button
        android:id="@+id/button59"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView25"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="3dp"
        android:background="@android:color/transparent"
        android:text=">" />

</RelativeLayout>