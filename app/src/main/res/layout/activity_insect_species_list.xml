<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:fillViewport="true"
    android:background="#FFFFFF">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffff"
    tools:context=".InsectSpeciesList">

    <Button
        android:id="@+id/button32"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="0dp"
        android:background="#86e2e9"
        android:text="Ants"
        android:textColor="#ffff"
        android:textSize="25dp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button34"
        android:layout_width="383dp"
        android:layout_height="61dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:background="#fdf6f6"
        android:text="Black House Ant"
        android:textSize="15dp"
        android:textStyle="bold" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="130dp"
        android:background="#f2f1f1" />

    <Button
        android:id="@+id/button36"
        android:layout_width="383dp"
        android:layout_height="61dp"
        android:layout_below="@+id/view2"
        android:background="#fdf6f6"
        android:text="Fire Ant"
        android:textSize="15dp"
        android:textStyle="bold" />

    <View
        android:id="@+id/view3"
        android:layout_width="383dp"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="194dp"
        android:background="#f2f1f1" />

    <Button
        android:id="@+id/button37"
        android:layout_width="383dp"
        android:layout_height="61dp"
        android:layout_below="@+id/view3"
        android:background="#fdf6f6"
        android:text="Ghost Ant"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="258dp"
        android:background="#f2f1f1" />

    <Button
        android:id="@+id/button38"
        android:layout_width="383dp"
        android:layout_height="61dp"
        android:layout_below="@+id/view4"
        android:background="#fdf6f6"
        android:text="Odorous House Ant"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <View
        android:id="@+id/view5"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="242dp"
        android:background="#f2f1f1" />

    <Button
        android:id="@+id/button42"
        android:layout_width="383dp"
        android:layout_height="61dp"
        android:layout_below="@+id/view5"
        android:layout_marginTop="-243dp"
        android:background="#fdf6f6"
        android:text="Pharaoh's Ant"
        android:textSize="15dp"
        android:textStyle="bold"/>

    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="384dp"
        android:background="#f2f1f1" />

    <Button
        android:id="@+id/button47"
        android:layout_width="28dp"
        android:layout_height="46dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="1dp"
        android:layout_marginLeft="1dp"
        android:layout_marginTop="12dp"
        android:background="@android:color/transparent"
        android:text=">"
        android:textSize="15dp" />

</RelativeLayout>
</ScrollView>